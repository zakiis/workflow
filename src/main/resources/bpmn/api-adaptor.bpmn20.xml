    <sequenceFlow id="flow10" name="request success" sourceRef="exclusivegateway1" targetRef="parallelgateway2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${requestResult == 'OK'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask4" name="cancelProcess" activiti:class="com.zakiis.workflow.service.task.CancelProcessTask"></serviceTask>
    <sequenceFlow id="flow11" name="request fail" sourceRef="exclusivegateway1" targetRef="servicetask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${requestResult != 'OK'}]]></conditionExpression>
    </sequenceFlow>
